{
    "climate.csv": "",
    "site.json": "",
    "crop.json": "",
    "climate.csv-options": {
        "csv-separator": ",",
        "no-of-climate-file-header-lines": 1,
        "header-to-acd-names": {
            "W_DATE": "iso-date",
            "RAIN": "precip",
            "TMAX": "tmax",
            "TMIN": "tmin",
            "SRAD": "globrad",
            "WIND": "wind",
            "RHAVD": "relhumid"
        }
    }, 
    "debug?": false,
    "output": {
        "write-file?": true,
        "path-to-output": "",
        "file-name": "sim-large.csv",
        "obj-outputs?": true,
        "csv-options": {
            "include-header-row": true, 
            "include-units-row": true, 
            "csv-separator": ","
        }, 
        "events": [
            {"start": "Sowing", "end": "Harvest"}, [
                "Date",
                "Crop",
                "Stage",
                "LAI",
                "Yield",
                "AbBiom|CWAD",
                "Tra|TRANS",
                "Act_ET|ETa",
                "RunOff|Roff",
                "PercolationRate|DPER",
                "NLeach|NLEA",
                ["Mois|SWC", [1, 20]]
            ],
            "crop", [
                ["LAI|LAID", "MAX"],
                "Yield",
                ["AbBiom|CWAD", "LAST"],
                ["OrgBiom|RWAD", "ROOT", "LAST"],
                ["OrgBiom|GWAD", "FRUIT", "LAST"]
            ]
        ]
    }, 
    "include-file-base-path": "data/",
    "NitrogenResponseOn": true,
    "EmergenceMoistureControlOn": true,
    "WaterDeficitResponseOn": true,
    "EmergenceFloodingControlOn": true,
    "NumberOfLayers": 20,
    "LayerThickness": [0.1, "m"],
    "UseSecondaryYields": true,
    "UseAutomaticIrrigation": false,
    "UseNMinMineralFertilisingMethod": false
}
